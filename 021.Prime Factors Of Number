public class PrimeFactorsOfNumber {
    public static void main(String[] args) {
        int num = 60;  // You can change this number
        int n = num;

        System.out.print("Prime factors of " + num + " are: ");

        for (int i = 2; i <= n; i++) {
            while (num % i == 0) {
                System.out.print(i + " ");
                num /= i;
            }
        }
    }
}
